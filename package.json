{
  "name": "online-store",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "webpack-dev-server --mode development --open",
    "build": "cross-env NODE_ENV=production webpack --mode production",
    "dev": "cross-env NODE_ENV=development webpack --mode development --watch",
    "lint": "eslint . --ext \"src/**/*.{jsx,ts,tsx,css}\"",
    "lint:fix": "eslint . --ext \"src/**/*.{jsx,ts,tsx,css}\" --fix",
    "format": "npx prettier --write \"src/**/*.{jsx,ts,tsx,css}\"",
    "prepare": "husky install",
    "execute-husky": "npx husky install && npx husky add .husky/pre-commit \"npx --no-install lint-staged\""
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.ts": "npm run lint:fix"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/DimaRK1997/Online-Store.git"
  },
  "author": "dima",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/DimaRK1997/Online-Store/issues"
  },
  "homepage": "https://github.com/DimaRK1997/Online-Store#readme",
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.42.1",
    "@typescript-eslint/parser": "^5.42.1",
    "clean-webpack-plugin": "^4.0.0",
    "cross-env": "^7.0.3",
    "css-loader": "^6.7.1",
    "eslint": "^8.27.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "html-webpack-plugin": "^5.5.0",
    "husky": "^8.0.2",
    "lint-staged": "^13.0.3",
    "prettier": "^2.7.1",
    "style-loader": "^3.3.1",
    "terser-webpack-plugin": "^5.3.6",
    "ts-loader": "^9.4.1",
    "typescript": "^4.8.4",
    "webpack": "^5.75.0",
    "webpack-cli": "^4.10.0",
    "webpack-dev-server": "^4.11.1"
  }
}
