{
    "parser": "@typescript-eslint/parser",
    "plugins": ["@typescript-eslint"],
    "extends": ["plugin:@typescript-eslint/recommended", "plugin:prettier/recommended"],
    "parserOptions": {
      "ecmaVersion": 2020,
      "sourceType": "module"
    },
    "env": {
      "es6": true,
      "browser": true
    },
    "rules": {
      "@typescript-eslint/no-explicit-any": 2,
      "no-console": 2
    }
  }